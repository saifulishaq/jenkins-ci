{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "learn-jenkins-app-prod",
    "containerDefinitions": [
        {
            "name": "learn-jenkins-app",
            "image": "nginx:1.26-alpine",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "name": "nginx-port-mapping",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512 MiB",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::977083441270:role/ecsTaskExecutionRole"
}